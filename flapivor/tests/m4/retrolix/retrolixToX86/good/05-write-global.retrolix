external printf

globals (x)
  lx : x <- copy "The universe is big, it's vast and complicated, and ridiculous and sometimes, very rarely, impossible things just happen and we call them miracles.";
  l00: %rdi <- copy "%s\n";
  l01: %rsi <- copy x;
  l02: printf();
  lx2: x <- copy "Like an assignment that works, for instance!";
  l03: %rdi <- copy "%s\n";
  l04: %rsi <- copy x;
  l05: printf();
  l06: exit;
end