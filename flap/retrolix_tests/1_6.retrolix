def fact(n)
l0 : %rax <- copy 1;
  %rdi <- copy n;
l1 : jumpif lte %rdi, 1 -> l5, l2;
l2 : %rax <- mul %rax, %rdi;
l3 : %rdi <- sub %rdi, 1;
l4 : jump l1;
l5 : %rdi <- copy %rdi;
end

globals(x, y)
  local n :
  n <- copy 5;
  fact(n);
  %rdi <- copy %rax;
  x <- copy %rdi;
end

globals()
  %rdi <- copy x;
  observe_int ();
end